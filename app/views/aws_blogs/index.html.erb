<p id="notice"><%= notice %></p>

<h1>Aws Blogs</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @aws_blogs.each do |aws_blog| %>
      <tr>
        <td><%= aws_blog.title %></td>
        <td><%= aws_blog.content %></td>
        <td><%= link_to 'Show', aws_blog %></td>
        <td><%= link_to 'Edit', edit_aws_blog_path(aws_blog) %></td>
        <td><%= link_to 'Destroy', aws_blog, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Aws Blog', new_aws_blog_path %>
